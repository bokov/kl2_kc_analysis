@article{Miguez-BurbanoInfluenceDifferentTypes2009,
  title = {The {{Influence}} of {{Different Types}} of {{Alcoholic Beverages}} on {{Disrupting Highly Active Antiretroviral Treatment}} ({{HAART}}) {{Outcome}}},
  volume = {44},
  copyright = {Published by Oxford University Press on behalf of the Medical Council on Alcohol 2009},
  issn = {0735-0414, 1464-3502},
  doi = {10.1093/alcalc/agp024},
  abstract = {Aims: Studies have yielded conflicting results regarding alcohol's influence on HIV outcomes, particularly after highly active antiretroviral treatment (HAART). Discrepant findings may be related to confounding variables, including gender, patterns of alcohol abuse and type of alcohol beverage beyond the amount consumed. Methods: Using a cohort study, differences in HAART effectiveness after 24 weeks of therapy were compared as a function of amount and preference for alcohol, drinking only liquor (LI, n = 55) or only wine or beer (BW, n = 110). Given the critical role of thymus on HAART response, changes in thymus size, CD4s, na\"ive lymphocytes and viral loads were assessed. Results: After HAART, positive increases in both CD4s (+12 cell counts/mm3) and thymus size (+0.7 mm3) were evident in the BW group. In contrast, the LI subgroup exhibited a decline in both parameters (-4 CD4 cells/mm3 and -0.6 mm3 in thymus size). Women in the LI group exhibited significantly lower CD4 (163.4 $\pm$ 46.2) and na\"ive counts (178 $\pm$ 69.5) than LI men (CD4: 281.6 $\pm$ 203, P = 0.05; lymphocytes: 301.4 $\pm$ 198, P = 0.04). In adjusted regression models, the LI compared to the BW subgroup had greater odds of maintaining detectable viral loads (RR = 1.35, 95\% CI 1.04\textendash{}1.75; P = 0.03), increased thymus volumes (RR = 3.8, P = 0.04) and replenished na\"ive cells (RR = 13, P = 0.02). Conclusions: Liquor was associated with thymus deterioration and thus with poorer viro-immune outcomes after HAART. Subtyping participants by alcohol consumption patterns seems to be clinically relevant and needs to be accounted for in future studies.},
  language = {en},
  number = {4},
  journal = {Alcohol and Alcoholism},
  author = {{M\'iguez-Burbano}, Mar\'ia Jos\'e and Lewis, John E. and Fishman, Joel and Asthana, Deshratn and Malow, Robert M.},
  month = jul,
  year = {2009},
  pages = {366-371},
  file = {/home/a/Zotero/storage/NCXERASP/Míguez-Burbano et al. - 2009 - The Influence of Different Types of Alcoholic Beve.pdf;/home/a/Zotero/storage/KZQHTXFB/366.html},
  pmid = {19454401}
}

