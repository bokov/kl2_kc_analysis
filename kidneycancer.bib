  pages = {545-557},
  file = {/home/a/Zotero/storage/E5X4EVU6/Helleringer et al. - 2014 - Cohort Profile The Likoma Network Study (LNS).pdf},
  pmid = {23543589},
  pmcid = {PMC3997370}
}

