@article{McKinnonHIV1CladeAssociated2012,
  title = {{{HIV}}-1 {{Clade D Is Associated}} with {{Increased Rates}} of {{CD4 Decline}} in a {{Kenyan Cohort}}},
  volume = {7},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0049797},
  abstract = {HIV-1 is grouped phylogenetically into clades, which may impact rates of HIV-1 disease progression. Clade D infection in particular has been shown to be more pathogenic. Here we confirm in a Nairobi-based prospective female sex worker cohort (1985\textendash{}2004) that Clade D (n$\mkern1mu$=$\mkern1mu$54) is associated with a more rapid CD4 decline than clade A1 (n$\mkern1mu$=$\mkern1mu$150, 20.6\% vs 13.4\% decline per year, 1.53-fold increase, p$\mkern1mu$=$\mkern1mu$0.015). This was independent of ``protective'' HLA and country of origin (p$\mkern1mu$=$\mkern1mu$0.053), which in turn were also independent predictors of the rate of CD4 decline (p$\mkern1mu$=$\mkern1mu$0.026 and 0.005, respectively). These data confirm that clade D is more pathogenic than clade A1. The precise reason for this difference is currently unclear, and requires further study. This is first study to demonstrate difference in HIV-1 disease progression between clades while controlling for protective HLA alleles.},
  number = {11},
  journal = {PLoS ONE},
  author = {McKinnon, Lyle R. and Nagelkerke, Nico J. and Kaul, Rupert and Shaw, Souradet Y. and Capina, Rupert and Luo, Ma and Kariri, Anthony and Apidi, Winnie and Kimani, Makobu and Wachihi, Charles and Jaoko, Walter and Anzala, A. Omu and Kimani, Joshua and Ball, T. Blake and Plummer, Francis A.},
  month = nov,
  year = {2012},
  file = {/home/a/Zotero/storage/BSRPZPBB/McKinnon et al. - 2012 - HIV-1 Clade D Is Associated with Increased Rates o.pdf},
  pmid = {23185441},
  pmcid = {PMC3504142},
  annote = {Different clades associated with different virulence.

Genetic diversity of the virus

Recombinant forms

~

"An early study in Kenya showed that clade C was associated with faster progression, higher viral loads, and lower CD4 counts than clades A1 or D~\href{http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3504142/#pone.0049797-Neilson1}{[4]}. Another study in Senegal demonstrated that infection by clades other than A1 was associated with an increased risk of disease progression during the period of follow-up\href{http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3504142/#pone.0049797-Kanki1}{[5]}. Recent studies in Kenya, Tanzania, and Uganda suggest that clade D is associated with more rapid disease progression than clade A, ranging from approximately 1.3 to $>$6 times faster~\href{http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3504142/#pone.0049797-Baeten1}{[6]},~\href{http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3504142/#pone.0049797-Kiwanuka1}{[7]},~\href{http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3504142/#pone.0049797-Vasan1}{[8]},~\href{http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3504142/#pone.0049797-Kaleebu1}{[9]}. It is unclear what immunological or virological mechanism(s) underlie these differences~\href{http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3504142/#pone.0049797-Kuritzkes1}{[10]}. One report suggested that clade D was associated with CXCR4-tropism~\href{http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3504142/#pone.0049797-Kaleebu2}{[11]}, which has previously been associated with faster progression to AIDS~\href{http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3504142/#pone.0049797-Connor1}{[12]}. \\
"}
}

