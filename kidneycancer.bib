  month = feb,
  year = {2013},
  keywords = {Statistics,evolutionary computation,image segmentation,boundary-based segmentation,deformable model,Deformable models,differential evolution,diversity function,energy functional,ETAN extension,ETAN optimization method,frequency memory population generator,global search memetic framework,image partitioning,internal energy term,local search,medical image,multistart local search,numerical optimization problem,Object segmentation,Optimization,region-based segmentation,scatter search,search problems,segmentation metrics,topological active nets},
  pages = {16-32},
  file = {/home/a/Zotero/storage/T9ZUA832/Bova et al. - 2013 - Image Segmentation Using Extended Topological Acti.pdf;/home/a/Zotero/storage/CTE2PFJ6/articleDetails.html}
