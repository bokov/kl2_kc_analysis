  pages = {159-164},
  file = {/home/a/Zotero/storage/PCT3KGKS/Malebranche et al. - 2010 - Exploring the “Bisexual Bridge” A Qualitative Stu.pdf},
  pmid = {19910348},
  pmcid = {PMC2791236}
}

